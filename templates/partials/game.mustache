{{#games}}
  <a class="card" href="https://firecup.ca/">
    <section class="card">
      <h2 class="red">{{title}}</h2>
      {{description}}
      {{#links.length}}
				<div class="links">
					{{#links}}
						<a href="{{url}}"><img src="img/link.png" alt=""/><p>{{label}}</p></a>
					{{/links}}
				</div>
			{{/links.length}}
    </section>
  </a>
{{/games}}
